{
  "name": "tourze/psr15-symfony-request-request-handler",
  "description": "PSR-15 Symfony Request Handler",
  "type": "library",
  "license": "MIT",
  "require": {
    "nyholm/psr7": "^1.8.2",
    "psr/http-message": "^1.1 || ^2.0",
    "psr/http-server-handler": "^1.0",
    "psr/log": "^3|^2|^1",
    "symfony/http-foundation": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/psr-http-message-bridge": "^7.3",
    "tourze/backtrace-helper": "0.1.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/framework-bundle": "^7.3"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\PSR15SymfonyRequestHandler\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\PSR15SymfonyRequestHandler\\Tests\\": "tests/"
    }
  }
}