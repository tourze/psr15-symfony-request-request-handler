{
  "name": "tourze/psr15-symfony-request-request-handler",
  "description": "PSR-15 Symfony Request Handler",
  "type": "library",
  "require": {
    "php": "^8.1",
    "psr/http-message": "^1.1 || ^2.0",
    "psr/http-server-handler": "^1.0",
    "nyholm/psr7": "^1.8.2",
    "symfony/http-kernel": "^6.4",
    "symfony/http-foundation": "^6.4",
    "symfony/psr-http-message-bridge": "^6.4",
    "tourze/backtrace-helper": "~0.0.5",
    "psr/log": "^3|^2|^1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Tourze\\PSR15SymfonyRequestHandler\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\PSR15SymfonyRequestHandler\\Tests\\": "tests/"
    }
  }
}